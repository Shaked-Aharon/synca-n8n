{
    "name": "n8n-nodes-synca",
    "version": "1.0.21",
    "description": "Custom API node for synca using n8n with CRUD operations",
    "keywords": [
        "n8n-community-node-package"
    ],
    "license": "MIT",
    "homepage": "https://n8n.synca.co.il",
    "author": {
        "name": "Synca Software Solutions",
        "email": "info@synca.co.il"
    },
    "repository": {
        "type": "git",
        "url": ""
    },
    "publishConfig": {
        "access": "public"
    },
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc && gulp build:icons && gulp build:move",
        "dev": "tsc --watch",
        "format": "prettier nodes credentials --write",
        "lint": "eslint nodes credentials package.json",
        "lintfix": "eslint nodes credentials package.json --fix",
        "prepublishOnly": "npm run build",
        "prepublishOnly2": "npm run build && npm run lint -s"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "credentials": [
            "dist/credentials/SyncaApi.credentials.js"
        ],
        "nodes": [
            "dist/nodes/Cashcow/SyncaCashcow.node.js",
            "dist/nodes/Priority/SyncaPriority.node.js",
            "dist/nodes/Superpharm/SyncaSuperpharm.node.js",
            "dist/nodes/Wolt/SyncaWolt.node.js"
        ]
    },
    "devDependencies": {
        "@typescript-eslint/parser": "^5.45.0",
        "gulp": "^5.0.1",
        "n8n-workflow": "*",
        "prettier": "^2.7.1",
        "typescript": "^4.8.4"
    },
    "peerDependencies": {
        "n8n-workflow": "*"
    }
}
